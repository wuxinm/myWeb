doctype html
html
	head
		meta(charset='uft-8')
		title= title
	
		//Mobile Specific Meta
		meta(name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1")

		//Stylesheets
		link(rel='stylesheet', href='/stylesheets/blog.css')
		link(rel='stylesheet', href='/stylesheets/bootstrapCSS/bootstrap.min.css')

		//JS
		script(src='/javascripts/jquery-1.11.0.min.js')
		script(src='/javascripts/bootstrapJS/bootstrap.min.js')
		script(src='/javascripts/jquery.gmap.min.js')
		script(src='https://maps.googleapis.com/maps/api/js?sensor=false')
		script(src='/javascripts/jquery.stellar.js')
	body
		//NAVBAR
		nav.navbar.navbar-inverse.navbar-fixed-top(role='navigation')
			.navbar-header
				button.navbar-toggle(type='button', data-toggle='collapse', data-target='#navbar-contents')
					span.sr-only Toggle navigation
					span.icon-bar
					span.icon-bar
					span.icon-bar
				a.navbar-brand.active(href='/')= info.brand_nav

			.collapse.navbar-collapse#navbar-contents
				ul.nav.navbar-nav(role='navigation')
					li: a(href='/#about_me')= info.home_nav
					li: a(href='/#skills')= info.skills_nav
					li.active: a(href='/blog')= info.blog_nav
					li: a(href='/#photos')= info.photos_nav
					li: a(href='/#demos')= info.demo_nav
					li: a(href='#')= info.cn_nav
		block content