section.row.skills#skills(data-stellar-background-ratio="0.5")
	.section-header.col-xs-12
		h1= info.skills.title
		h2= info.skills.subtitle
		h3.icon-header.col-xs-8.col-xs-offset-2
			span.glyphicon.glyphicon-tasks
	.content.col-xs-10.col-xs-offset-1
		p= info.skills.intro
		.ex_block
			.col-md-3.col-xs-12.group1
				h3= info.skills.progress_1
				h5 60%
				.progress
					.progress-bar.progress-bar-success(role='progressbar' aria-valuenow='60' aria-valuemin='0' aria-valuemax='100' style='width: 60%')
						span.sr-only 60% Complete
			.col-md-3.col-xs-12.group1
				h3= info.skills.progress_2
